package life.genny.rules;

import life.genny.rules.QRules;


rule "Send Treeview To Frontend"
    no-loop true
    when
        rules: QRules( isState("LAYOUT_CHANGE_READY")) 
     then
     	rules.header();
     	
      	rules.publishBaseEntitysByParentAndLinkCode("GRP_ROOT","LNK_CORE",0,10,true);
   	    rules.println("Root Group is "+"GRP_ROOT");
     		
	    rules.footer();     		      
end
