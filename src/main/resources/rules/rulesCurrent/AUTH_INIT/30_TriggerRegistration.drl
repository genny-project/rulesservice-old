package life.genny.rules;

import life.genny.rules.QRules;


rule "Trigger Registration"
    no-loop true
    salience 570
    when
        rules: QRules(  (isState("NEW_REGISTRATION")) && (!isState("TRIGGER_REGISTRATION")))
     then
      	rules.setState("TRIGGER_REGISTRATION");
   		drools.setFocus("Registration");
		
	      		      
end
