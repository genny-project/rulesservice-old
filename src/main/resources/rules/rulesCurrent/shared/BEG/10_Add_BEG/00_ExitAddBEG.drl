package life.genny.rules;
import life.genny.rules.QRules;

rule "Exit Add BEG"
	when
		rules: QRules( 	isState("SENT_ADD_BEG_LAYOUT") && 
						!isState("LOOP_EXIT_ADD_BEG") ) 
	 then
	 	rules.header();
        rules.setState("LOOP_EXIT_ADD_BEG");
        
		rules.setState("EXITED_ADD_BEG");
	 	rules.footer();      
end
