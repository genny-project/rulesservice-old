package life.genny.rules;

import life.genny.rules.QRules;

rule "Send Bucket Data"
    when
        rules: QRules( isState("SEND_BUCKET_DATA") && !isState("LOOP_SENT_BUCKET_DATA_CH40") )
     then
     	rules.header();

     	rules.setState("LOOP_SENT_BUCKET_DATA_CH40");
		rules.sendLayoutsAndData();
	    rules.footer();
end
