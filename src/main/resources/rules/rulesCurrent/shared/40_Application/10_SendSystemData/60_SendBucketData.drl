package life.genny.rules;
import life.genny.rules.QRules;

rule "Send Bucket Data"
    when
        rules: QRules(  isState("LOOP_SEND_TREE_DATA") && 
                        !isState("LOOP_SEND_BUCKET_DATA")  ) 
     then

        rules.header();
        rules.setState("LOOP_SEND_BUCKET_DATA");
  		rules.sendLayoutsAndData();	
	    rules.footer();     		      
end
