package life.genny.rules;
import life.genny.rules.QRules;

rule "Send Tree Data"
    when
        rules: QRules(  isState("SEND_TREE_DATA") &&
                        !isState("LOOP_SEND_TREE_DATA")  )
     then

        rules.header();
        rules.setState("LOOP_SEND_TREE_DATA");
  		  rules.sendTreeData();
        rules.setState("SEND_BUCKET_DATA");

	      rules.footer();
end
