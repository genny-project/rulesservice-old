rule "Profile is NOT completed - step 3"
    when
	    rules: QRules( isState("PROFILE_NOT_COMPLETED") && !isState("LOOP_PROFILE_NOT_COMPLETED") )
     then
     	rules.header();

		rules.setState("LOOP_PROFILE_NOT_COMPLETED");
		rules.setState("TRIGGER_REGISTRATION");
    rules.setState("IS_REGISTRATION");

	  rules.footer();
end
