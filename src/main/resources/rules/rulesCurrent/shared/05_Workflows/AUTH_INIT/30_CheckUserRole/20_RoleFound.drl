package life.genny.rules;
import life.genny.rules.QRules;

rule "Role Found"
     when
        rules: QRules(  isState("ROLE_FOUND") &&
                        !isState("LOOP_DID_FIND_ROLE") )
     then
        rules.header();
		rules.setState("LOOP_DID_FIND_ROLE");

        rules.setState("TRIGGER_PROFILE_CHECK");
      	rules.footer();
end
