package life.genny.rules;

import life.genny.rules.QRules;

rule "Exit Profile check - channel40"
    when
        rules: QRules(isState("PROFILE_CHECK_COMPLETED") &&
                    !isState("DID_EXIT_ROLE_CHECK") )
     then
        rules.setState("DID_EXIT_ROLE_CHECK");
        rules.footer();
end
