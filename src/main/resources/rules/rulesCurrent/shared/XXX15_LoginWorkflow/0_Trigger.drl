package life.genny.rules;

import life.genny.rules.QRules;
import life.genny.qwanda.entity.BaseEntity;

rule "Login Workflow"
    when
         rules: QRules( isState("TRIGGER_LOGIN_WORKFLOW") && !isState("LOOP_LOG_IN") ) 
     then
     	rules.header();
		rules.setState("LOOP_LOG_IN");
        rules.startWorkflow("login");
	    rules.footer();      		      
end
