package life.genny.rules;
import life.genny.rules.QRules;

rule "Send Tree Data"
    when
        rules: QRules(  isState("TREE_DATA_CHANNEL40") &&
                        !isState("LOOP_SEND_TREE_DATA")  )
     then

        rules.header();
        rules.setState("LOOP_SEND_TREE_DATA");
  		  rules.sendTreeData();

        rules.setState("BUCKET_DATA_CHANNEL40");
	      rules.footer();
end
