package life.genny.rules;
import life.genny.rules.QRules;

rule "send 5day reminder to EduProvider"
	when
		rules: QRules(	isState("MSG_INTERNMATCH_5_DAY_PLEASE_SIGN_AGREEMENT_EDU_PROVIDER_REP") &&
						!isState("EDU_PROVIDER_REP_SIGNED_AGREEMENT"))
	then
		rules.header(); 
		
		rules.footer();
end