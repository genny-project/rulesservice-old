package life.genny.rules;
import life.genny.rules.QRules;

rule "Send Internmatch BucketData To Frontend"
    when
        rules: QRules(isState("PROFILE_IS_COMPLETED") && 
                    !isState("SEND_BUCKET_DATA"))
     then
     	rules.header();

     	rules.setState("SEND_BUCKET_DATA");
		rules.sendInternMatchLayoutsAndData();
		
	    rules.footer();
end
