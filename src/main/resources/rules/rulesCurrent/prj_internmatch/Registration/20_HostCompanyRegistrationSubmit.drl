package life.genny.rules;

import life.genny.rules.QRules;


rule "Host Company Registration Submit"
    when
        rules: QRules( !isState("HOST_COMPANY_REGISTRATION_SUBMITTED") && isState("QUE_HOST_COMPANY_GRP"))
     then
     	rules.header();
     	rules.setState("HOST_COMPANY_REGISTRATION_SUBMITTED");
     	rules.setState("PROFILE_IS_COMPLETED");
	    rules.footer();
end
