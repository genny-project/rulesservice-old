package life.genny.rules;

import life.genny.rules.RulesUtils;
import life.genny.rules.QRules;

rule "Handle Role Selection Mentormatch"
    when
    		m : QEventMessage( event_type.equals("BTN_CLICK") && data.code.equals("BTN_SELECT_ROLE"))
        rules: QRules( ) 
     then

     	rules.header();
    		rules.handleMentormatchRoleSelection(m);
	    rules.footer();
end
