package life.genny.rules;

import life.genny.rules.QRules;

rule "Pre Init"
    salience 60000
	no-loop true
    when
         rules: QRules(!isStarted()) 
     then    
     System.out.println("STARTING RULES FIRING");
        rules.setDrools(drools);
         rules.setStarted(true);
         rules.setState("STARTED");
       		      
end
